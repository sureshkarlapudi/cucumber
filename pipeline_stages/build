stage ("build") {

    echo "################### BUILD STAGE STARTED FOR ${BRANCH_NAME} #######################"
    
try {
       
           sh "/work/jenkins/apache-maven-3.5.0/bin/mvn package"
       

     
 }   catch (e) {
    currentBuild.result = "FAILED"
    
    throw e
 }

echo "################### BUILD STAGE END FOR ${BRANCH_NAME} #######################"
}


